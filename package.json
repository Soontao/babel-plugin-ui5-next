{
  "name": "babel-plugin-ui5-next",
  "version": "7.2.3",
  "description": "Next-generation syntax for SAP UI5",
  "repository": "Soontao/babel-plugin-ui5-next",
  "author": "Theo Sun <theo.sun@sap.com>",
  "main": "lib/index.js",
  "devDependencies": {
    "@babel/cli": "^7.0.0",
    "@babel/core": "^7.5.5",
    "@babel/helper-plugin-utils": "^7.0.0",
    "@babel/plugin-proposal-class-properties": "^7.0.0",
    "@babel/plugin-proposal-decorators": "^7.0.0",
    "@babel/plugin-proposal-do-expressions": "^7.0.0",
    "@babel/plugin-proposal-export-default-from": "^7.0.0",
    "@babel/plugin-proposal-export-namespace-from": "^7.0.0",
    "@babel/plugin-proposal-function-bind": "^7.0.0",
    "@babel/plugin-proposal-function-sent": "^7.0.0",
    "@babel/plugin-proposal-json-strings": "^7.0.0",
    "@babel/plugin-proposal-logical-assignment-operators": "^7.0.0",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.0.0",
    "@babel/plugin-proposal-numeric-separator": "^7.0.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.5.5",
    "@babel/plugin-proposal-optional-chaining": "^7.0.0",
    "@babel/plugin-proposal-pipeline-operator": "^7.0.0",
    "@babel/plugin-proposal-throw-expressions": "^7.0.0",
    "@babel/plugin-syntax-class-properties": "^7.0.0",
    "@babel/plugin-syntax-decorators": "^7.0.0",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/plugin-syntax-import-meta": "^7.0.0",
    "@babel/plugin-syntax-jsx": "^7.0.0",
    "@babel/plugin-syntax-typescript": "^7.3.3",
    "@babel/plugin-transform-arrow-functions": "^7.0.0",
    "@babel/plugin-transform-block-scoped-functions": "^7.0.0",
    "@babel/plugin-transform-block-scoping": "^7.0.0",
    "@babel/plugin-transform-classes": "^7.0.0",
    "@babel/plugin-transform-computed-properties": "^7.0.0",
    "@babel/plugin-transform-destructuring": "^7.0.0",
    "@babel/plugin-transform-duplicate-keys": "^7.0.0",
    "@babel/plugin-transform-for-of": "^7.0.0",
    "@babel/plugin-transform-function-name": "^7.0.0",
    "@babel/plugin-transform-literals": "^7.0.0",
    "@babel/plugin-transform-modules-commonjs": "^7.5.0",
    "@babel/plugin-transform-object-assign": "^7.0.0",
    "@babel/plugin-transform-parameters": "^7.0.0",
    "@babel/plugin-transform-react-jsx": "^7.0.0",
    "@babel/plugin-transform-shorthand-properties": "^7.0.0",
    "@babel/plugin-transform-spread": "^7.0.0",
    "@babel/plugin-transform-sticky-regex": "^7.0.0",
    "@babel/plugin-transform-template-literals": "^7.4.4",
    "@babel/plugin-transform-typescript": "^7.6.0",
    "@babel/plugin-transform-unicode-regex": "^7.0.0",
    "@babel/preset-env": "^7.5.5",
    "@babel/register": "^7.5.5",
    "@commitlint/cli": "^8.1.0",
    "@commitlint/config-conventional": "^7.0.1",
    "babel-jest": "^25.1.0",
    "babel-plugin-transform-async-await": "^1.0.0",
    "babel-plugin-transform-async-to-promises": "^0.8.14",
    "babel-plugin-transform-es2015-typeof-symbol": "^6.6.0",
    "husky": "^1.1.2",
    "jest": "^25.1.0",
    "standard-version": "^7"
  },
  "dependencies": {
    "lodash": "^4.17.15"
  },
  "scripts": {
    "build": "babel src -d lib",
    "test": "jest",
    "test:watch": "npm run test -- --watch",
    "prepare": "npm run build",
    "release": "standard-version --commit-all --no-verify && git push --follow-tags origin master",
    "commitmsg": "commitlint -E HUSKY_GIT_PARAMS",
    "precommit": "npm run build",
    "coverage": "npm test -- --coverage --no-cache"
  },
  "keywords": [
    "UI5",
    "babel",
    "plugin",
    "Open",
    "babel-plugin"
  ],
  "jest": {
    "testTimeout": 30000,
    "transform": {
      "^.+\\.[t|j]sx?$": "babel-jest"
    },
    "collectCoverageFrom": [
      "src/**/*.js",
      "!**/node_modules/**"
    ],
    "coveragePathIgnorePatterns": [
      "node_modules/",
      "test/",
      "lib/"
    ],
    "testEnvironment": "node",
    "testRegex": "/test/index.js$"
  },
  "license": "MIT"
}
